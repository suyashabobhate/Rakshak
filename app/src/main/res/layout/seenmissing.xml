<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/crime_login_page">

    <View
        android:id="@+id/view"
        android:layout_width="410dp"
        android:layout_height="282dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="73dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SEEN MISSING PERSON"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        app:fontFamily="@font/butcherman"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="6dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WHEN"
        android:textColor="#FFFFFF"
        app:fontFamily="@font/butcherman"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="378dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WHERE"
        android:textColor="#FFFFFF"
        app:fontFamily="@font/butcherman"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="422dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONDITION"
        android:textColor="#FFFFFF"
        app:fontFamily="@font/butcherman"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="466dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="88dp"
        android:layout_height="92dp"
        android:text="ANY ADDITIONAL INFORMATION"
        android:textColor="#FFFFFF"
        app:fontFamily="@font/butcherman"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="513dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="639dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="222dp"
        android:layout_height="31dp"
        android:ems="10"
        android:inputType="time"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="378dp"
        tools:ignore="MissingConstraints" />


    <fragment
        android:id="@+id/autocomplete_fragment"
        android:layout_width="222dp"
        android:layout_height="31dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="422dp"
        tools:visibility="invisible" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="468dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="532dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>